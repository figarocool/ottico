	#tag Event
		Sub Open()
		  self.left = (screen(0).width/2)-(self.width/2)
		  self.top = (screen(0).Height/2)-(self.Height/2)
		  
		  txtRicerca.SetFocus
		  
		  dim rs as RecordSet
		  rs = Db.SQLSelect("select * from clienti")
		  while not rs.eof
		    lstclienti.AddRow deApici(rs.IdxField(2).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,1) = DeApici(rs.IdxField(3).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,2) = DeApici(rs.IdxField(4).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,3) =  Sesso(rs.IdxField(5).IntegerValue)
		    lstclienti.cell(lstclienti.ListCount-1,4) = DeApici(rs.IdxField(6).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,5) = DeApici(rs.IdxField(7).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,6) = DEApici(rs.IdxField(8).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,7) = DeAPici(rs.IdxField(9).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,8) = DeApici(rs.IdxField(10).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,9) = DeApici(rs.IdxField(11).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,10) = DeApici(rs.IdxField(12).StringValue)
		    lstclienti.RowTag(lstclienti.ListCount-1) =rs.IdxField(1).StringValue
		    rs.movenext
		  wend
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub Ricerca()
		  
		  dim rs as RecordSet
		  
		  if rbTutti.Value = False then
		    rs = db.SQLSelect("Select * from clienti where  cognome like '%"+apici(txtRicerca.text)+"%' ")
		  else
		    rs = db.SQLSelect("Select * from clienti where [cognome] like '%"+apici(txtRicerca.text)+"%' or [nome] like  '%"+apici(txtRicerca.text)+"%' or [codice fiscale] like  '%"+apici(txtRicerca.text)+"%' or [sesso] like  '%"+apici(txtRicerca.text)+"%' or [indirizzo] like  '%"+apici(txtRicerca.text)+"%' or [cap] like  '%"+apici(txtRicerca.text)+"%' or [Localita] like  '%"+apici(txtRicerca.text)+"%' or [Provincia] like  '%"+apici(txtRicerca.text)+"%' or [Telefono] like  '%"+apici(txtRicerca.text)+"%' or [SecTel] like  '%"+apici(txtRicerca.text)+"%' or [Professione] like  '%"+apici(txtRicerca.text)+"%' or [Oculista] like  '%"+apici(txtRicerca.text)+"%'")
		  end if
		  lstclienti.DeleteAllRows
		  while not rs.eof
		    lstclienti.AddRow DeApici(rs.IdxField(2).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,1) = DeApici(rs.IdxField(3).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,2) = DeApici(rs.IdxField(4).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,3) =  Sesso(rs.IdxField(5).IntegerValue)
		    lstclienti.cell(lstclienti.ListCount-1,4) = DeApici(rs.IdxField(6).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,5) = DeApici(rs.IdxField(7).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,6) = DEApici(rs.IdxField(8).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,7) = DeAPici(rs.IdxField(9).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,8) = DeApici(rs.IdxField(10).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,9) = DeApici(rs.IdxField(11).StringValue)
		    lstclienti.cell(lstclienti.ListCount-1,10) = DeApici(rs.IdxField(12).StringValue)
		    lstclienti.RowTag(lstclienti.ListCount-1) =rs.IdxField(1).StringValue
		    rs.movenext
		  wend
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Function Sesso(Tipo as integer) As string
		  if Tipo = 0 then
		    return "M"
		  else
		    return "F"
		  end if
		  
		End Function
	#tag EndMethod



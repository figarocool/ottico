#tag Events BevelButton7
	#tag Event
		Sub Action()
		  if lstclienti.ListIndex <> -1 then
		    dim tmp as Integer
		    tmp = MsgBox ("Sei davvero sicuro di voler eliminare il seguente Cliente ?",36,"Software ottico")
		    if tmp = 6 then
		      
		      db.SQLExecute("delete  from [clienti] where idclienti="+lstclienti.RowTag(lstclienti.ListIndex)+"")
		      db.Commit
		      db.SQLExecute("delete  from [dati-tecnici-misurazioni] where idcliente="+lstclienti.RowTag(lstclienti.ListIndex)+"")
		      db.Commit
		      db.SQLExecute("delete  from [dati-tecnici-montature] where idcliente="+lstclienti.RowTag(lstclienti.ListIndex)+"")
		      db.Commit
		      db.SQLExecute("delete  from [dati-tecnici-lenti] where idcliente="+lstclienti.RowTag(lstclienti.ListIndex)+"")
		      db.Commit
		      call lstclienti.RemoveRow(lstclienti.ListIndex)
		      MsgBox "Cliente Eliminato."
		    end if
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
